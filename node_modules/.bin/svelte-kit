#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/r3dux/Repositories/ai-stocks/client/node_modules/.pnpm/@sveltejs+kit@2.5.2_@sveltejs+vite-plugin-svelte@3.0.2_svelte@5.0.0-next.68_vite@5.1.4/node_modules/@sveltejs/kit/node_modules:/Users/r3dux/Repositories/ai-stocks/client/node_modules/.pnpm/@sveltejs+kit@2.5.2_@sveltejs+vite-plugin-svelte@3.0.2_svelte@5.0.0-next.68_vite@5.1.4/node_modules/@sveltejs/node_modules:/Users/r3dux/Repositories/ai-stocks/client/node_modules/.pnpm/@sveltejs+kit@2.5.2_@sveltejs+vite-plugin-svelte@3.0.2_svelte@5.0.0-next.68_vite@5.1.4/node_modules:/Users/r3dux/Repositories/ai-stocks/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/r3dux/Repositories/ai-stocks/client/node_modules/.pnpm/@sveltejs+kit@2.5.2_@sveltejs+vite-plugin-svelte@3.0.2_svelte@5.0.0-next.68_vite@5.1.4/node_modules/@sveltejs/kit/node_modules:/Users/r3dux/Repositories/ai-stocks/client/node_modules/.pnpm/@sveltejs+kit@2.5.2_@sveltejs+vite-plugin-svelte@3.0.2_svelte@5.0.0-next.68_vite@5.1.4/node_modules/@sveltejs/node_modules:/Users/r3dux/Repositories/ai-stocks/client/node_modules/.pnpm/@sveltejs+kit@2.5.2_@sveltejs+vite-plugin-svelte@3.0.2_svelte@5.0.0-next.68_vite@5.1.4/node_modules:/Users/r3dux/Repositories/ai-stocks/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
fi
